
write("How deep?")
local Num = tonumber( read() )

for i=1,Num,1 do
  write("Loop i iter["..tostring(i).."]\n")
  
  for j=1,5,1 do
  	write("Loop j iter["..tostring(j).."]\n")
    turtle.dig()
	turtle.forward()
  end
  
  for k=1,4,1 do
    write("Loop k iter["..tostring(k).."]\n")
    turtle.back()
  end
  
  turtle.digDown()
  turtle.down()
end